{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "forin": false,
    "interface-name": [
      false
    ],
    "no-console": [
      false
    ],
    "no-namespace": {
      "severity": "warning"
    },
    "trailing-comma": [
      true,
      "never"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "one-line": [
      false,
      "check-catch",
      "check-finally",
      "check-else",
      "check-open-brace"
    ],
    "ordered-imports": false,
    "no-trailing-whitespace": [
      true,
      "ignore-blank-lines"
    ],
    "object-literal-sort-keys": false,
    "align": [
      false,
      "parameters"
    ],
    "max-line-length": {
      "severity": "warning",
      "options": 90
    },
    "arrow-return-shorthand": {
      "severity": "warning"
    },
    "only-arrow-functions": false,
    "no-shadowed-variable": {
      "severity": "warning"
    },
    "member-ordering": false,
    "no-conditional-assignment": {
      "severity": "warning"
    },
    "radix": false,
    "jsdoc-format": false,
    "whitespace": false,
    "no-empty-interface": {
      "severity": "warning"
    },
    "no-empty": {
      "severity": "warning"
    }
  }
}
